{
  "security.workspace.trust.untrustedFiles": "open",
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "explorer.confirmDragAndDrop": false,
  "git.openRepositoryInParentFolders": "never",
  "workbench.startupEditor": "none",
  "git.confirmSync": false,
  "git.autofetch": true,
  "editor.fontFamily": "Berkeley Mono",
  "editor.cursorStyle": "block",
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false
  },
  "GitHooks.viewContainerDisplay": false,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "workbench.activityBar.location": "bottom",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "mdmath.autosave": true,
  "mdmath.theme": "publication",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.allowedUNCHosts": [
    "wsl.localhost"
  ],
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "debug.javascript.autoAttachFilter": "disabled",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "explorer.confirmDelete": false,
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Firefox Dark",
  "cursor-ruler.color": "#00ff80",
  "cursor-ruler.enabled": false,
  "cursor-ruler.width": "0px",
  "indenticator.color.dark": "#F80",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "errorLens.gutterIconSet": "emoji",
  "errorLens.ignoreUntitled": true,
  "errorLens.respectUpstreamEnabled": {
    "statusBar": true
  },
  "errorLens.messageBackgroundMode": "message",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.fontSize": "13",
  "errorLens.delayMode": "debounce",
  "errorLens.messageTemplate": "[$code] - from $source: $message",
  "errorLens.fontWeight": "600",
  "errorLens.statusBarColorsEnabled": true,
  "eslint.debug": true,
  "errorLens.statusBarMessageEnabled": true,
  "git.enableSmartCommit": true,
  "cSpell.userWords": [
    "Akshit",
    "amerika",
    "apologised",
    "apologising",
    "assed",
    "batshit",
    "behaviour",
    "Beyonce",
    "choiceless",
    "colour",
    "colours",
    "Corto",
    "cuck",
    "cucked",
    "demonised",
    "desireless",
    "duvidha",
    "falsifiably",
    "Ghalib",
    "Godin",
    "Goldmouth",
    "GREGOR",
    "journaling",
    "kahin",
    "Kleinberg",
    "Kolkata",
    "Malorum",
    "Mukti",
    "nahin",
    "normalise",
    "Osho's",
    "ostracised",
    "parameterless",
    "perciipi",
    "pluripotentiality",
    "Premeditatio",
    "PTSD",
    "rationalise",
    "realisation",
    "realise",
    "realised",
    "realising",
    "recognise",
    "Rogan",
    "SAMSA",
    "Sharma",
    "Shunryu",
    "shūnya",
    "situationship",
    "snoozefest",
    "Suraj",
    "traumatised",
    "undecide",
    "undecision",
    "vyavahar",
    "بھرم",
    "بیدار",
    "تیری",
    "جاتے",
    "رکھتے",
    "محفل",
    "ورنہ",
    "गालिब",
    "रखने"
  ],
  "editor.largeFileOptimizations": false,
  "code-runner.clearPreviousOutput": true,
  "code-runner.enableAppInsights": false,
  "code-runner.showExecutionMessage": false,
  "cSpell.enableFiletypes": [
    "!javascript",
    "markdown_latex_combined"
  ],
  "cSpell.language": "en,en-GB",
  "cSpell.files": [
    "*.js",
    "*.ts"
  ],
  "notebook.lineNumbers": "on",
  "git.ignoreRebaseWarning": true,
  "glassit.alpha": 255,
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.iconTheme": "file-icons",
  "workbench.preferredDarkColorTheme": "Blowington Theme (Dark)",
  "chat.commandCenter.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "workbench.layoutControl.enabled": false,
  "window.menuBarVisibility": "toggle",
  "workbench.secondarySideBar.showLabels": false,
  "errorLens.statusBarIconsEnabled": true,
  "solidity.telemetry": false,
  "files.autoSave": "onWindowChange",
  "editor.formatOnSave": true,
  "window.newWindowProfile": "Default"
}